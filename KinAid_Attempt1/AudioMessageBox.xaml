<Window x:Class="KinAid_Attempt1.AudioMessageBox"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="KinAid" Height="150" Width="300" ResizeMode="NoResize" ShowInTaskbar="False" WindowStartupLocation="CenterOwner">
    <StackPanel>
        <Label Height="80" Name="message" />
        <Grid Height="30">
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Left">
                <Image Source="Voice.bmp" />
                <Label Content="Yes" BorderBrush="Black" BorderThickness="1" />
            </StackPanel>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                <Image Source="Voice.bmp" />
                <Label Content="No" BorderBrush="Black" BorderThickness="1" />
            </StackPanel>
        </Grid>
    </StackPanel>
</Window>
